public class OrderedSuperArray extends SuperArray{
    public OrderedSuperArray(){
	//Complete this.
    }

    /*call the proper add.*/
    public void add(int index, String value){
	add(value);
    }

    /*Write this method and any
    appropriate helper methods.*/
    private int findIndex(String[] arr,String str){
	for(int i=arr.length-1;i>=0;i-=1){
	    if(str.comparesto(arr[i-1])>0){
		return i;
	    }
	    return 0;
	}
    }
    public boolean add(String value){
	//add to the correct spot.
	//you may still use super.add(index,value)
	super.add(findIndex(data,value),value);
	return true;
    }
}
